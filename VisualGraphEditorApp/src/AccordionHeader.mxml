<?xml version="1.0" encoding="utf-8"?>
<mx:Button xmlns:fx="http://ns.adobe.com/mxml/2009" 
				xmlns:s="library://ns.adobe.com/flex/spark" 
				xmlns:mx="library://ns.adobe.com/flex/mx" 
				mouseOver="itemrenderer1_mouseOverHandler(event)">
	
	<fx:Script>
		<![CDATA[
			protected function itemrenderer1_mouseOverHandler(event:MouseEvent):void
			{
				var clickEvent:MouseEvent = new MouseEvent(MouseEvent.CLICK, true, false, event.localX, event.localY);
				dispatchEvent(clickEvent);
			}
		]]>
	</fx:Script>
	
</mx:Button>
