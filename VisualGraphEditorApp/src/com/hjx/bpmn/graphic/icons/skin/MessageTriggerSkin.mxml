<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns="http://ns.adobe.com/mxml/2009"
		xmlns:s="library://ns.adobe.com/flex/spark">

  <Metadata>
    [HostComponent("com.hjx.bpmn.graphic.icons.BPMNIcon")]
  </Metadata>

  <s:Group>
    <s:Path data="M 0.00 0.00 L 16.20 0.00 L 16.20 10.80 L 0.00 10.80 Z">
      <s:stroke><s:SolidColorStroke caps="none" color="{hostComponent.closedStrokeColor}" weight="1"/></s:stroke>
      <s:fill><s:SolidColor alpha="{hostComponent.filled ? 1 : 0}" color="{hostComponent.fillColor}"/></s:fill>
    </s:Path>
    <s:Path data="M 0.00 0.00 L 8.10 6.30 L 16.20 0.00">
      <s:stroke><s:SolidColorStroke caps="none" color="{hostComponent.openStrokeColor}" weight="1"/></s:stroke>
    </s:Path>
  </s:Group>

</s:Skin>
