		xmlns:editor="com.hjx.diagram.editor.*" xmlns:primatives="ws.tink.spark.primatives.*" xmlns:graphics="ws.tink.spark.graphics.*">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("com.hjx.diagram.editor.ResizableNodeAdorner")]
	</fx:Metadata>
	<primatives:Rect left="-4" right="-4" top="-4" bottom="-4">
		<primatives:stroke>
			<graphics:SolidColorDash  color="0x00A8FF"/>
		</primatives:stroke>
	</primatives:Rect>
	<editor:ResizeHandle id="topLeftHandle" left="-8" top="-8"
				   skinClass="com.hjx.diagram.editor.skin.HandleSkin"/>
	<editor:ResizeHandle id="topRightHandle" right="-8" top="-8"  
				   skinClass="com.hjx.diagram.editor.skin.HandleSkin"/>
