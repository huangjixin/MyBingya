<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:mx="library://ns.adobe.com/flex/mx"
		xmlns:editor="com.hjx.diagram.editor.*"
		xmlns:primatives="ws.tink.spark.primatives.*"
		xmlns:graphics="ws.tink.spark.graphics.*">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("com.hjx.diagram.editor.LinkAdorner")]
	</fx:Metadata>
	<editor:LinkConnectionHandle id="startHandle" x="{hostComponent.startHandleX}"
								 y="{hostComponent.startHandleY}"
								 rotation="{hostComponent.startHandleRotation}"
								 skinClass="com.hjx.diagram.editor.skin.LinkConnectionHandleSkin"/>
								 
	<editor:LinkConnectionHandle id="endHandle" x="{hostComponent.endHandleX}"
								 y="{hostComponent.endHandleY}"
								 rotation="{hostComponent.endHandleRotation}"
								 skinClass="com.hjx.diagram.editor.skin.LinkConnectionHandleSkin"/>
								 
</s:Skin>
